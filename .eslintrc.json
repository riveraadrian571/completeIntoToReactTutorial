{
	"extends": [
		"plugin:flowtype/recommended",
		"airbnb",
		"prettier",
		"prettier/react"
	],
	"plugins": [
		"flowtype",
		"prettier"
	],
	"parser": "babel-eslint",
	"parserOptions": {
		"ecmaVersion": 2016,
		"sourceType": "module"
	},
	"env": {
		"es6": true,
		"browser": true,
		"node": true,
		"jest": true
	},
	"rules": {
		"linebreak-style": 0
	}
}